    "@remix-run/react": "^1.1.3",
    "@remix-run/server-runtime": "^1.2.2",
    "@tailwindcss/forms": "^0.4.0",
    "@types/lodash.debounce": "^4.0.6",
    "algosdk": "^1.15.0",
    "base64-js": "^1.5.1",
    "bottleneck": "^2.19.5",
    "buffer": "^6.0.3",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "decorate-all": "^1.1.1",
    "leaflet": "^1.7.1",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.5",
    "remix": "^1.1.3",
    "tailwindcss": "^3.0.18",
    "tslib": "^2.3.1",
    "twitter-api-v2": "^1.11.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@tailwindcss/forms": "^0.4.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.1.2"
